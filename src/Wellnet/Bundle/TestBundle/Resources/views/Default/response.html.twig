{% extends "::base.html.twig" %}

{% block content %}
    <script>
        $(document).ready(function () {
            $('#response').html('<pre>' + JSON.stringify({{ data.body|raw }}, null, '  ') + '</pre>');
        });
    </script>

    <div class="page-header">
        <h1>{{ app.request.query.get('method') }}</h1>
    </div>

    <p><b>Code</b>: {{ data.statusCode }}</p>
    <p><b>Headers</b>: {{ data.getRawHeaders }}</p>
    <p><b>Response</b>:</p>
    <div id="response" style="background-color:#ccc; padding:10px; width: 1000px"></div>
{% endblock %}
